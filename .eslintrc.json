{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "globals": {
        "__dirname": "readonly",
        "process": "readonly",
        "_": "readonly",
        "MathJax":"writable"
    },
    "rules": {
        "no-unused-vars":"warn"
    }
}
