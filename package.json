{
  "name": "latexmathhelper",
  "version": "0.1.1",
  "description": "latex math environment input helper",
  "main": "app/main.js",
  "scripts": {
    "start": "electron . ",
    "dist": "electron-builder",
    "style": "eslint app/*.js",
    "test": "mocha",
    "validate": "npm run style && npm run test"
  },
  "build": {
    "appId": "io.a358003542.latexmathhelper",
    "productName": "latexmathhelper",
    "directories": {
      "buildResources": "resources"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a358003542/LatexMathHelper.git"
  },
  "keywords": [
    "latex",
    "math"
  ],
  "author": {
    "name": "wanze",
    "email": "a358003542@outlook.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a358003542/LatexMathHelper/issues"
  },
  "homepage": "https://github.com/a358003542/LatexMathHelper#readme",
  "devDependencies": {
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "eslint": "^8.5.0",
    "mocha": "^9.1.3"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "mathjax-full": "^3.2.0"
  }
}
